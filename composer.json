{
    "name": "shamarkellman/auth-logger",
    "description": "Log all authentication events performed with an application",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Shamar Kellman",
            "email": "shamarkellman@hotmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/auth": "~5.5",
        "illuminate/bus": "~5.5",
        "illuminate/console": "~5.5",
        "illuminate/contracts": "~5.5",
        "illuminate/database": "~5.5",
        "illuminate/http": "~5.5",
        "illuminate/notifications": "~5.5",
        "illuminate/support": "~5.5"
    },
    "autoload": {
        "psr-4": {
            "Shamarkellman\\AuthLogger\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shamarkellman\\AuthLogger\\Providers\\AuthLoggerServiceProvider"
            ],
            "aliases": {
                "SimplePackage": "Shamarkellman\\AuthLogger\\Facades\\AuthLoggerFacade"
            }
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
